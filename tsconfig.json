{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"types": ["jest"],
		"paths": {
			// name and path to aliases
		   "@atoms": ["src/lib/atoms"],
		   "@molecules": ["src/lib/molecules"],
	   }
	}
}
